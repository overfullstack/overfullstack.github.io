  "description": "It's all Over full-stack",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "test": "test --passWithNoTests",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "prettier": "prettier \"**/*.{md,css,scss,yaml,yml}\"",
    "lint": "eslint --cache --ext .js,.jsx --ignore-pattern public ."
  },
  "repository": "git+https://github.com/overfullstack/overfullstack.github.io.git,",
  "author": "Gopal S Akshintala <gopal.akshintala@gmail.com>",
